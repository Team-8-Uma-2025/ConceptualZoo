{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "frontend/build",
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "runtime": "@vercel/node@3.0.0"
      }
    }
  }