{
    "version": 2,
    "buildCommand": "cd frontend && CI=false npm run build",
    "outputDirectory": "frontend/build",
    "builds": [
      { "src": "api/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.js" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }